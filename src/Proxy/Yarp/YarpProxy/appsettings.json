{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "seq": "http://seq"
  },
  "ReverseProxy": {
    "Routes": {
      "chatRoute": {
        "ClusterId": "chat",
        "Match": {
          "Path": "/chat/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/chat"
          }
        ]
      },
      "notificationRoute": {
        "ClusterId": "notification",
        "Match": {
          "Path": "/notification/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/notification"
          }
        ]
      },
      "photosRoute": {
        "ClusterId": "photos",
        "Match": {
          "Path": "/photos/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/photos"
          }
        ]
      },
      "tasksRoute": {
        "ClusterId": "tasks",
        "Match": {
          "Path": "/tasks/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/tasks"
          }
        ]
      },
      "api.aggregatorRoute": {
        "ClusterId": "api.aggregator",
        "Match": {
          "Path": "/aggregator/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/aggregator"
          }
        ]
      },
      "projectsRoute": {
        "ClusterId": "projects",
        "Match": {
          "Path": "/projects/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/projects"
          }
        ]
      },
      "signalrRoute": {
        "ClusterId": "signalr",
        "Match": {
          "Path": "/hub/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "chat": {
        "Destinations": {
          "chat/destination1": {
            "Address": "http://chat:80"
          }
        }
      },
      "notification": {
        "Destinations": {
          "notification/destination1": {
            "Address": "http://notification:80"
          }
        }
      },
      "photos": {
        "Destinations": {
          "photos/destination1": {
            "Address": "http://photos:80"
          }
        }
      },
      "tasks": {
        "Destinations": {
          "tasks/destination1": {
            "Address": "http://tasks:80"
          }
        }
      },
      "api.aggregator": {
        "Destinations": {
          "api.aggregator/destination1": {
            "Address": "http://api.aggregator:80"
          }
        }
      },
      "projects": {
        "Destinations": {
          "projects/destination1": {
            "Address": "http://projects:80"
          }
        }
      },
      "signalr": {
        "Destinations": {
          "signalr/destination1": {
            "Address": "http://signalr:80"
          }
        }
      }
    }
  }

}
